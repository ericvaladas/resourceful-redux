{
  "name": "resourceful-redux",
  "version": "0.0.9",
  "description": "A powerful Redux framework for interacting with remote resources.",
  "main": "dist/resourceful-redux.js",
  "scripts": {
    "test": "gulp",
    "lint": "gulp lint",
    "watch": "gulp watch",
    "build": "gulp clean && gulp build && NODE_ENV=production gulp build",
    "coverage": "gulp coverage",
    "prepublish": "npm run build",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:prepare": "gitbook install",
    "docs:clean": "rimraf _book",
    "docs:build": "npm run docs:prepare && gitbook build",
    "docs:publish": "npm run docs:clean && npm run docs:build && cp CNAME _book && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:jmeas/resourceful-redux gh-pages --force"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jmeas/resourceful-redux.git"
  },
  "keywords": [
    "redux",
    "api",
    "boilerplate",
    "resource",
    "restful",
    "rest",
    "react",
    "flux",
    "framework",
    "crud",
    "http",
    "xhr",
    "state",
    "data",
    "store",
    "reducer",
    "action",
    "type",
    "creator"
  ],
  "author": "James Smith <jellyes2@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmeas/resourceful-redux/issues"
  },
  "files": [
    "dist",
    "prop-types",
    "action-creators"
  ],
  "homepage": "https://github.com/jmeas/resourceful-redux",
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "6.13.0",
    "babel-preset-latest": "6.14.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "6.14.0",
    "chai": "3.5.0",
    "del": "2.2.2",
    "gitbook-cli": "^2.3.0",
    "glob": "7.0.6",
    "gulp": "3.9.1",
    "gulp-eslint": "3.0.1",
    "gulp-filter": "4.0.0",
    "gulp-istanbul": "1.1.1",
    "gulp-livereload": "3.8.1",
    "gulp-load-plugins": "1.2.4",
    "gulp-mocha": "3.0.1",
    "gulp-plumber": "1.1.0",
    "gulp-rename": "1.2.2",
    "gulp-sourcemaps": "1.6.0",
    "gulp-uglify": "2.0.0",
    "isparta": "4.0.0",
    "json-loader": "0.5.4",
    "lodash": "^4.17.4",
    "mocha": "3.0.2",
    "pleaserc": "^2.3.1",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "webpack": "2.6.1",
    "webpack-stream": "^3.2.0"
  }
}
